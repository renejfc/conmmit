{
  "version": "0.0.2-alpha.0",
  "name": "conmmit",
  "main": "build/index.js",
  "author": {
    "name": "ReneJFC",
    "url": "https://x.com/ReneJFC_"
  },
  "keywords": [
    "commit",
    "conventional commits",
    "cli",
    "git"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/renejfc/conmmit.git"
  },
  "scripts": {
    "start": "bun ./src/index.ts",
    "build": "bun build --target=bun ./src/index.ts --outdir ./build",
    "format": "biome format . --write",
    "typecheck": "tsc",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --apply",
    "magic:fix": "biome check . --apply"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/bun": "latest",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.0.0"
  },
  "type": "module",
  "bin": {
    "conmmit": "build/index.js"
  }
}
